{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "doc",
			"problemMatcher": [
				"$rustc"
			],
			"group": "build",
			"label": "rust: cargo doc"
		},
		{
			"type": "cargo",
			"command": "llvm-cov",
			"problemMatcher": [
				"$rustc"
			],
			"args": [
				"--html"
			],
			"group": "test",
			"label": "rust: cargo llvm-cov"
		}
	]
}
